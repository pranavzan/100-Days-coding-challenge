#include <stdio.h>
#define MAX_SIZE 100
void mergeSortedArrays(int nums1[], int m, int nums2[], int n, int merged[]) {
    int i = 0, j = 0, k = 0;

    while (i < m && j < n) {
        if (nums1[i] < nums2[j]) {
            merged[k++] = nums1[i++];
        } else {
            merged[k++] = nums2[j++];
        }
    }

    while (i < m) {
        merged[k++] = nums1[i++];
    }

    while (j < n) {
        merged[k++] = nums2[j++];
    }
}
int main() {
    int nums1[MAX_SIZE], nums2[MAX_SIZE], merged[MAX_SIZE * 2];
    int m, n;

    printf("Enter the number of elements in the first sorted array: ");
    scanf("%d", &m);

    printf("Enter the elements of the first sorted array:\n");
    for (int i = 0; i < m; i++) {
        scanf("%d", &nums1[i]);
    }

    printf("Enter the number of elements in the second sorted array: ");
    scanf("%d", &n);

    printf("Enter the elements of the second sorted array:\n");
    for (int i = 0; i < n; i++) {
        scanf("%d", &nums2[i]);
    }

    mergeSortedArrays(nums1, m, nums2, n, merged);

    printf("The merged sorted array is:\n");
    for (int i = 0; i < m + n; i++) {
        printf("%d ", merged[i]);
    }
    printf("\n");

    return 0;
}
