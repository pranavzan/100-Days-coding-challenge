#include <stdio.h>
int main() {
    int m, n;  
    int p, q;  
    printf("Enter the number of rows for the first matrix: ");
    scanf("%d", &m);
    printf("Enter the number of columns for the first matrix: ");
    scanf("%d", &n);
    int A[m][n];  
    printf("Enter elements of the first matrix:\n");
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < n; j++) {
            scanf("%d", &A[i][j]);
        }
    }
    printf("Enter the number of rows for the second matrix: ");
    scanf("%d", &p);
    printf("Enter the number of columns for the second matrix: ");
    scanf("%d", &q);
    if(n != p) { 
        printf("Matrix multiplication is not possible: columns of first matrix (%d) must equal rows of second matrix (%d).\n", n, p);
        return 0;
    }
    int B[p][q];
    printf("Enter elements of the second matrix:\n");
    for(int i = 0; i < p; i++) {
        for(int j = 0; j < q; j++) {
            scanf("%d", &B[i][j]);
        }
    }
    int C[m][q];
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < q; j++) {
            C[i][j] = 0;
            for(int k = 0; k < n; k++) { 
                C[i][j] += A[i][k] * B[k][j];
            }
        }
    }
    printf("Resulting matrix after multiplication:\n");
    for(int i = 0; i < m; i++) {
        for(int j = 0; j < q; j++) {
            printf("%d ", C[i][j]);
        }
        printf("\n");
    }
    return 0;
}
