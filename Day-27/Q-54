#include <stdio.h>
int main() {
    int n = 4;
    for (int i = 1; i <= 2 * n - 1; i++) {
        int num_stars = 2 * (i <= n ? i : (2 * n - i)) - 1;
        int num_spaces = (2 * n - 1 - num_stars) / 2;
            for (int j = 1; j <= num_spaces; j++) {
            printf(" ");
        }
        for (int j = 1; j <= num_stars; j++) {
            printf("*");
        }
        printf("\n");
    }
    return 0;
}
