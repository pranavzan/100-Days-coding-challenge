#include <stdio.h>
#include <stdlib.h>  
#include <string.h>  
int main() {
    char input[100]; 
    int digitCount[10] = {0};
    int i, maxCount = 0, mostFrequentDigit = -1;
    printf("Enter an integer: ");
    fgets(input, sizeof(input), stdin);  
    size_t len = strlen(input);
    if (len > 0 && input[len-1] == '\n') {
        input[len-1] = '\0';
    }
    for (i = 0; input[i] != '\0'; i++) {
        if (input[i] >= '0' && input[i] <= '9') {  
            int digit = input[i] - '0';  
        }
        }
    
    for (i = 0; i < 10; i++) {
        if (digitCount[i] > maxCount) {
            maxCount = digitCount[i];
            mostFrequentDigit = i;
        }
    }
    if (maxCount > 0) {
        printf("The most frequent digit is '%d' which occurs %d times.\n", mostFrequentDigit, maxCount);
    } else {
        printf("No digits found in the input.\n");
    }
    return 0;
}
