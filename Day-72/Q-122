#include <stdio.h>
int main() {
    FILE *file;
    char line[100];

    // Opening the existing file in read mode
    file = fopen("info.txt", "r");
    if (file == NULL) {
        printf("Error opening file!\n");
        return 1;
    }

    // Reading and printing each line until EOF
    while (fgets(line, sizeof(line), file) != NULL) {
        printf("%s", line);
    }

    // Closing the file
    fclose(file);

    return 0;
}
